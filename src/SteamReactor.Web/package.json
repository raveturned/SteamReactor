        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^5.1.2",
        "redux": "^4.0.5",
        "redux-saga": "^0.16.2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/preset-react": "^7.9.1",
        "@babel/preset-stage-3": "^7.8.3",
        "babel-loader": "^8.1.0",
        "babel-plugin-es6-promise": "^1.1.1",
        "babel-polyfill": "^6.26.0",
        "css-loader": "^1.0.1",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-loader": "^2.2.1",
        "eslint-plugin-import": "^2.20.1",
